class Sum:
    def dodaj(self, x, y):
        print 'Brak implementacji'
    def __init__(self, start=[  ]):
        self.data = start
    def __dodaj__(self, other):
        return self.dodaj(self.data, other)
                          
class ListSum(Sum):             
    def dodaj(self, a, b):
        return a + b            # Konkatenacja

class SlowSum(Sum):
    def dodaj(self, x, y):
        nowy = {  }
        for k in x.keys(  ): nowy[k] = x[k]
        for k in y.keys(  ): nowy[k] = y[k]
        return nowy
    
print ListSum().dodaj('maj', 'ster')

print SlowSum().dodaj('x', 'y')