a01 = ['Kowalski', '90', ('schabowy', 'krupnik')]
a02 = ['Nowak', '80', ('jajecznica', 'pomidorowa')]
a03 = ['Iksinski', '75', ('chleb', 'barszcz')]
a04 = ['Nowakowski', '85', ('ryz', 'bigos')]
a05 = ['Kowal', '75', ('bulka', 'kebab')]
a06 = ['Smith', '95', ('makaron', 'KFC')]
a07 = ['Wisniewski', '72', ('zeberka', 'pizza')]
a08 = ['Dabrowski', '83', ('pizza', 'tosty')]
a09 = ['Lewandowski', '68', ('hot-dog', 'bigos')]
a10 = ['Wojcik', '76', ('zupa', 'kaczka')]

def sortowanie((x1, y1), (x2, y2)):
    if y1 > y2:
        return 1
    elif y1 == y2:
        return 0
    else:
        return -1
 
a01.sort(sortowanie)