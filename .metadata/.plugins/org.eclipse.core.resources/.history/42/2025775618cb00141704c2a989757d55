a = int(raw_input("Podaj dlugosc wektora a: "))

def lslownie(liczba):
    """Liczba całkowita słownie"""
    trojki = []
    if liczba == 0:
        return u("zero")
    while liczba > 0:
        trojki.append(liczba % 1000)
        liczba = liczba // 1000
    slowa = []
    for i, n in enumerate(trojki):
        if n > 0:
            if i > 0:
                p = _przypadek(n)
                w = WIELKIE[i][p]
                slowa.append(_slownie3cyfry(n) + u(" ") + w)
            else:
                slowa.append(_slownie3cyfry(n))
    slowa.reverse()
    return u(" ").join(slowa)