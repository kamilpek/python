from math import sqrt
from math import floor

x = int(raw_input("Podaj liczbe: "))

# --- SPOSÓB PIERWSZY ---
jest = False
y = 1
while (y < x) and not jest:
    if (y * y) == x:
        jest = True
    y += 1
    
if jest:
    print 'TAK'
else:
    print 'NIE' 
    
# --- SPOSÓB DRUGI ---
n = sqrt(x)
print n    
print n.is_integer()

# --- SPOSÓB TRZECI ---
math.floor(n) == n

